{
    "name": "Ruby Development",
    "dockerFile": "Dockerfile",
    "extensions": [
      "rebornix.Ruby",
      "kaiwood.endwise",
      "groksrc.ruby",
      "hoovercj.ruby-linter",
      "miguel-savignano.ruby-symbols",
      "wingrunr21.vscode-ruby"
    ],
    "settings": {
      "[ruby]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
      },
    },
    "postCreateCommand": "bundle install"
   }